{
    "name": "ewamvscadaptor",
    "displayName": "eWamVSCAdaptor",
    "description": "e-Wam Visual Studio Adaptor",
    "version": "0.0.2",
    "publisher": "mphasiswyde",
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:ewam.openEntity",
        "onCommand:ewam.checkIn",
        "onCommand:ewam.checkOut",
        "onCommand:ewam.parse",
        "onCommand:ewam.reimplem",
        "onCommand:ewam.scenario",
        "onCommand:ewam.deliver",
        "onCommand:ewam.newClass",
        "onCommand:ewam.metaInfo",
        "onCommand:ewam.run"
    ],
    "main": "./extension",
    "contributes": {
        "snippets": [
            {
                "language": "gold",
                "path": "./snippets/snippets.json"
            }
        ],
        "keybindings": [
            {
                "command": "ewam.parse",
                "key": "F7",
                "mac": "F7",
                "when": "editorTextFocus"
            },
            {
                "command": "ewam.scenario",
                "key": "alt+s",
                "mac": "alt+s",
                "when": "editorTextFocus"
            },
            {
                "command": "ewam.checkIn",
                "key": "alt+i",
                "mac": "alt+i",
                "when": "editorTextFocus"
            },
            {
                "command": "ewam.checkOut",
                "key": "alt+c",
                "mac": "alt+c",
                "when": "editorTextFocus"
            },
            {
                "command": "ewam.openEntity",
                "key": "alt+o",
                "mac": "alt+o",
                "when": "editorTextFocus"
            },
            {
                "command": "ewam.reimplem",
                "key": "alt+r",
                "mac": "alt+r",
                "when": "editorTextFocus"
            },
            {
                "command": "ewam.deliver",
                "key": "alt+d",
                "mac": "alt+d",
                "when": "editorTextFocus"
            },
            {
                "command": "ewam.newClass",
                "key": "alt+n",
                "mac": "alt+n"
            },
            {
                "command": "ewam.metaInfo",
                "key": "alt+m",
                "mac": "alt+m"
            },
            {
                "command": "ewam.run",
                "key": "F5",
                "mac": "F5"
            }
        ],
        "commands": [
            {
                "command": "ewam.openEntity",
                "title": "eWam Open Entity"
            },
            {
                "command": "ewam.checkIn",
                "title": "eWam Check In"
            },
            {
                "command": "ewam.checkOut",
                "title": "eWam Check Out"
            },
            {
                "command": "ewam.parse",
                "title": "eWam Parse"
            },
            {
                "command": "ewam.reimplem",
                "title": "eWam Reimplem"
            },
            {
                "command": "ewam.scenario",
                "title": "eWam Scenario"
            },
            {
                "command": "ewam.deliver",
                "title": "eWam Deliver"
            },
            {
                "command": "ewam.newClass",
                "title": "eWam New Class"
            },
            {
                "command": "ewam.metaInfo",
                "title": "eWam Class Meta Info"
            },
            {
                "command": "ewam.run",
                "title": "eWam Run Application"
            }
        ],
        "languages": [
            {
                "id": "gold",
                "aliases": [
                    "Gold",
                    "gold"
                ],
                "extensions": [
                    ".gold",
                    ".god"
                ],
                "configuration": "./gold.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gold",
                "scopeName": "source.gold",
                "path": "./syntaxes/gold.tmLanguage"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "eWam configuration",
            "properties": {
                "ewam.url": {
                    "type": "string",
                    "default": "http://localhost:8082",
                    "description": "End point of your eWam as a service"
                },
                "ewam.className": {
                    "type": "string",
                    "default": "aWFApplicationLauncher",
                    "description": "Class used to launch the application"
                },
                "ewam.methodName": {
                    "type": "string",
                    "default": "Run",
                    "description": "Method used to launch the application"
                },
                "ewam.args": {
                    "type": "array",
                    "items": {
                        "type": "object"
                    },
                    "default": [{"key":"WFAPPLINAME","value":"WynsureSolution_Life_US"}, {"key":"RUNCONTEXTNAME","value":"MultiLanguageRunningContext"}],
                    "description": "Arguments used to launch the application"
                }
            }
        }
    },
    "devDependencies": {
        "vscode": "0.10.x"
    },
    "dependencies": {
        "axios": "^0.9.1",
        "fs": "0.0.2"
    }
}